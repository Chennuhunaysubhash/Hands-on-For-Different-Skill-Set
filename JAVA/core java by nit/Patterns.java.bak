import java.util.Scanner;
class Patterns 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.print("enter the n value: ");
		int n = sc.nextInt();
		for(int i=n;i>=1;i--)
		{
			for(int j=i; j<=n;j++)
			{
				System.out.print(j+" ");  
			}
			System.out.println();
		}
		System.out.println();
	}
}
